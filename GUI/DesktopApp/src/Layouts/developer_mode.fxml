<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UserInterface.DeveloperMode">
   <children>
      <Button fx:id="send_btn" layoutX="27.0" layoutY="438.0" mnemonicParsing="false" onAction="#sendCommand" prefHeight="50.0" prefWidth="200.0" text="Send">
         <font>
            <Font size="16.0" />
         </font></Button>
      <TextField fx:id="topic_txt" layoutX="526.0" layoutY="438.0" prefHeight="50.0" prefWidth="200.0" promptText="Topic name">
         <font>
            <Font size="16.0" />
         </font></TextField>
      <TextField fx:id="command_txt" layoutX="27.0" layoutY="141.0" prefHeight="50.0" prefWidth="412.0" promptText="Command">
         <font>
            <Font size="16.0" />
         </font></TextField>
      <Button fx:id="echo_btn" layoutX="769.0" layoutY="438.0" mnemonicParsing="false" onAction="#echoTopic" prefHeight="50.0" prefWidth="200.0" text="echo">
         <font>
            <Font size="16.0" />
         </font></Button>
      <TextArea fx:id="subbed_txt" disable="true" editable="false" layoutX="526.0" layoutY="141.0" prefHeight="200.0" prefWidth="443.0" promptText="Subscribed Data">
         <font>
            <Font size="16.0" />
         </font>
      </TextArea>
      <Button fx:id="back_btn" layoutX="769.0" layoutY="525.0" mnemonicParsing="false" onAction="#back" prefHeight="50.0" prefWidth="200.0" text="Back">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
   </children>
</Pane>
